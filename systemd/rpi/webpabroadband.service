[Unit]
Description=WebpaBroadBand
After=PsmSsp.service parodus.service CcspPandMSsp.service ccspwifiagent.service CcspLMLite.service
Requires=parodus.service 

[Service]
ExecStartPre=/bin/sh -c '/lib/rdk/webpa_pre_setup.sh;'
ExecStart=/usr/bin/webpa &
Type=forking
Restart=always

[Install]
WantedBy=multi-user.target

